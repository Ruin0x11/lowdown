<div id="page-header">
  <h1>Mark Complete</h1>
</div>

<%= form_tag :action=>:bulk_update, :method=>"POST" do  %>
  <fieldset>

    <p><strong>Mark all summaries complete for periods between:</strong> <%= date_select :update, :start_date %> and <%= date_select :update, :end_date %></p>

    <div class="actions">
      <%= submit_tag 'Update' %>
    </div>

  </fieldset>
<% end %>
