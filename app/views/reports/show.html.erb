<div id="page-header">
  <h1><% if @report.name %><%= @report.name %><% else %>Network Service Summary<% end %></h1>
</div>

<p>
  <%= report_month_range @start_date, @end_date %>
  <span class="separator csv">|</span>
  <%= link_to "CSV", params.merge({:action=>:csv}), :class=>"csv" %>
</p>

<%= content_tag :p, "Adjustments for #{report_month_range @report.adjustment_start_date, @report.adjustment_end_date}" if @report.adjustment? %>
<%= content_tag :p, @report.description if @report.description.present? %>

<%= render :partial=> "report", :locals=>{:results=>@results, :group_fields=>@group_fields} %>
