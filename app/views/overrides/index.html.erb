<ul id="subnav" class="selfclear">
  <li><%= link_to 'New Override', new_override_path, :class => :add if current_user.is_admin %></li>
</ul>
<div id="page-header">
  <h1>Listing Overrides</h1>
</div>

<table>
  <tr>
    <th>Name</th>
    <th></th>
  </tr>

  <% @overrides.each do |override| %>
    <tr>
      <td><%= link_to override.name, edit_override_path(override) %></td>
      <td><%= link_to "Delete", override_url(override), :class => "delete", :method => "delete", :confirm => "Are you sure you want to delete this override?" if current_user.is_admin && override.allocations.empty? %></td>
    </tr>
  <% end %>
</table>

<%= will_paginate @overrides %>
