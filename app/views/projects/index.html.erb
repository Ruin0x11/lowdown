<div id="page-header">
  <h1>Listing projects</h1>
</div>

<table>
  <tr>
    <th>Project Name</th>
    <th>Funding Source</th>
    <th>Funding Sub-Source</th>
    <th>Project Number</th>
  </tr>

<% @projects.each do |project| %>
  <tr>
    <td><%= link_to project.name, edit_project_path(project) %></td>
    <td><%= project.funding_source %></td>
    <td><%= project.funding_subsource %></td>
    <td><%= project.project_number %></td>
  </tr>
<% end %>
</table>

<%= will_paginate @projects %>

<%= link_to 'New Project', new_project_path, :class => "action-bttn" if current_user.is_admin %>
