<%= form_for(@trimet_report_group) do |form| %>
  <%= render "shared/error_messages", :target => @trimet_report_group %>
  
  <fieldset>
    <ol class="section ">
      <li class="selfclear">
        <%= form.label :name %>
        <%= form.text_field :name %>
      </li>
    </ol>
    
    <div class="actions">
      <%= form.submit %>
    </div>
  </fieldset>
<% end %>
