<div id="page-header">
  <h1>Create Initial User</h1>
</div>

<%= form_for @user, :url=>:init do |f| %>

Email:<br/>
<%= f.text_field :email %><br/>

Password:<br/>
<%= f.password_field :password %><br/>

Password confirmation:<br/>
<%= f.password_field :password_confirmation %><br/>

<%= f.submit "Create" %>

<% end %>
