<div class="report-wrapper">
  <table>
    <%= render "flex_reports/header", :report_group_fields => group_fields %>
    <tbody>
      <%= render "flex_reports/nested_rows", :rows => results, :report_group_fields => group_fields, :group_fields => group_fields %>
      <%= render 'flex_reports/totals', 
          :row=>results, :report_group_fields => group_fields, :group_fields=>group_fields, :grand_total => true %>
    </tbody>
  </table>
</div>
