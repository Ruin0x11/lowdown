<div id="page-header">
  <h1>Create Age And Ethnicity Report</h1>
</div>

<%= form_for @report, :as => :q, :html=>{:id=>"create-report", :method=>:get}, :url=>{:action=>"age_and_ethnicity"} do |f| %>

<fieldset>

  <p>
    <span><strong>Service for the Month:</strong></span> 
    <%= f.date_select :start_date, :discard_day=>true %>
    <%= f.collection_select :provider, @providers, :to_s, :to_s %>
  </p>

  <div class="actions">
    <input id="report-create" type="submit" value="Create Report" />
  </div>

</fieldset>

<% end %><!-- end form -->
